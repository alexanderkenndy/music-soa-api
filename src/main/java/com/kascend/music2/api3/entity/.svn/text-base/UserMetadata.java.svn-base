package com.kascend.music2.api3.entity;

import java.util.List;

public class UserMetadata extends BaseData {
	
	private String playlistTitle;
	private long playlistid;
	private String description;
	private long uid;
	
	private String playlistThumb;
	
	private List<UserSong> clientUserSongList;
	
	private int dataFrom;
	
	private int playlistType;

	public String getPlaylistTitle() {
		return playlistTitle;
	}

	public void setPlaylistTitle(String playlistTitle) {
		this.playlistTitle = playlistTitle;
	}

	public long getPlaylistid() {
		return playlistid;
	}

	public void setPlaylistid(long playlistid) {
		this.playlistid = playlistid;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public long getUid() {
		return uid;
	}

	public void setUid(long uid) {
		this.uid = uid;
	}

	public String getPlaylistThumb() {
		return playlistThumb;
	}

	public void setPlaylistThumb(String playlistThumb) {
		this.playlistThumb = playlistThumb;
	}


	public List<UserSong> getClientUserSongList() {
		return clientUserSongList;
	}

	public void setClientUserSongList(List<UserSong> clientUserSongList) {
		this.clientUserSongList = clientUserSongList;
	}

	public int getDataFrom() {
		return dataFrom;
	}

	public void setDataFrom(int dataFrom) {
		this.dataFrom = dataFrom;
	}

	public int getPlaylistType() {
		return playlistType;
	}

	public void setPlaylistType(int playlistType) {
		this.playlistType = playlistType;
	}
	
	
}	
