package com.kascend.music2.api3.service.metadata.info;

import java.util.List;

public class DiscoveryInfo extends PageInfo {
	
	private int albumartflag;
	private int sort=1;
	private String styles;
	private long playlistid;
	private List<Integer> styleList;
	public long getPlaylistid() {
		return playlistid;
	}
	public void setPlaylistid(long playlistid) {
		this.playlistid = playlistid;
	}
	
	public List<Integer> getStyleList() {
		return styleList;
	}
	public void setStyleList(List<Integer> styleList) {
		this.styleList = styleList;
	}
	public int getAlbumartflag() {
		return albumartflag;
	}
	public void setAlbumartflag(int albumartflag) {
		this.albumartflag = albumartflag;
	}
	public int getSort() {
		return sort;
	}
	public void setSort(int sort) {
		this.sort = sort;
	}
	public String getStyles() {
		return styles;
	}
	public void setStyles(String styles) {
		this.styles = styles;
	}
	
}
